{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect":"Allow",
      "Action":["s3:GetObject","s3:ListBucket"],
      "Resource":[
        "arn:aws:s3:::newsinsights-raw-340752797090-us-west-2",
        "arn:aws:s3:::newsinsights-raw-340752797090-us-west-2/*"
      ]
    },
    {
      "Effect":"Allow",
      "Action":["s3:PutObject","s3:ListBucket"],
      "Resource":[
        "arn:aws:s3:::newsinsights-processed-340752797090-us-west-2",
        "arn:aws:s3:::newsinsights-processed-340752797090-us-west-2/*"
      ]
    },
    {
      "Effect":"Allow",
      "Action":["ssm:GetParameter"],
      "Resource":[
        "arn:aws:ssm:us-west-2:340752797090:parameter/newsinsights/BEDROCK_MODEL_ID"
      ]
    },
    {
      "Effect":"Allow",
      "Action":["bedrock:InvokeModel","bedrock:InvokeModelWithResponseStream"],
      "Resource":"*"
    }
  ]
}
